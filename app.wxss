/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

page {
  font-size: 16px;
}

.clear {
  zoom: 1
}

.clear::after {
  display: block;
  clear: both;
  content: "";
  visibility: hidden;
  height: 0
}

.show {
  display: block;
}

.hidden {
  display: none;
}